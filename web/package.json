{
  "name": "web",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "tsx watch ./server.ts",
    "build": "tsc && yarn run build:client && yarn run build:server",
    "build:client": "vite build --outDir dist/client  --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "generate": "rm -rf dist; tsc && vite build --outDir dist/static --ssrManifest && yarn run build:server && tsx prerender.ts",
    "serve": "vite preview --outDir dist/static",
    "test": "playwright test"
  },
  "dependencies": {
    "@iconify/react": "^4.0.1",
    "@types/marked": "^4.0.8",
    "caniuse-lite": "^1.0.30001565",
    "dayjs": "^1.11.7",
    "dompurify": "^2.4.3",
    "jsdom": "^21.0.0",
    "marked": "^4.2.12",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0",
    "react-router-dom": "^6.6.2",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.2.1",
    "@playwright/test": "^1.47.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/dompurify": "^2.4.0",
    "@types/express": "^4.17.16",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsx": "^4.6.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
